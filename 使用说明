wget https://repo.anaconda.com/miniconda/Miniconda3-py37_4.9.2-Linux-aarch64.sh

bash Miniconda3-py37_4.9.2-Linux-aarch64.sh

vim ~/.bashrc

export  PATH="/root/miniconda3/bin:"$PATH

source ~/.bashrc

sudo chown root:root -R miniconda3

conda create -n python39 python=3.9

conda activate python39

conda deactivate

conda remove -n your_env_name(虚拟环境名称) --all

conda list

conda remove package_name

conda remove --name your_env_name package_name




git clone https://github.com/sml2h3/dddd_trainer.git

pip install -r requirements.txt -i https://pypi.douban.com/simple

pip3 install torch torchvision torchaudio --index-url https://download.pytorch.org/whl/cpu

python app.py create FunCaptcha
